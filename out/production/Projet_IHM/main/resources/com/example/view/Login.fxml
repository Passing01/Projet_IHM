<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import java.awt.Button?>
<?import java.awt.TextField?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.com.example.controller.LoginController">
    <TextField fx:id="usernameField" promptText="Nom d'utilisateur"/>
    <PasswordField fx:id="passwordField" promptText="Mot de passe"/>
    <Button fx:id="loginButton" text="Connexion" onAction="#handleLogin"/>
</VBox>
